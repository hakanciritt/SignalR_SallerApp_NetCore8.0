@model List<string>

@section Scripts {
    <script src="~/js/admin/admin_chat.js" asp-append-version="true"></script>

}
<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Ürünler</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">

                        <table class="table mt-3">
                            <thead>
                                <tr>
                                    <th scope="col">Ürün Adı</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

